version: "3.9"
services:
  proxy:
    image: nginx
    container_name: nginx
    networks: #_mode: "host"
      - open
    volumes:
      - ./conf:/etc/nginx/conf.d
      - ./cert:/data/identify
      - ./logs:/var/log/nginx
      - ./nginx.conf:/etc/nginx/nginx.conf
      - /etc/letsencrypt/:/etc/letsencrypt/
    ports:
      - 80:80
      - 443:443
      - 89:89
      - 90:90
      - 91:91
    restart: always
networks:
  open:
    external:
      name: open-network